{
  "name": "@tomsd/rand",
  "version": "3.1.5",
  "description": "functions returning random values",
  "main": "dist/cjs/rand.js",
  "module": "dist/esm/rand.js",
  "types": "dist/esm/rand.d.ts",
  "scripts": {
    "format": "npm run format:src && npm run format:test",
    "format:src": "prettier --write src/**/*.ts",
    "format:test": "prettier --write test/**/*.ts",
    "lint:src": "eslint src/**/*.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha -r ts-node/register \"test/test.ts\" --timeout 30000",
    "build": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsdoo/rand.git"
  },
  "keywords": [],
  "author": "tom",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "mocha": "^7.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
